@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    ViewBag.SemiTitle = "Dashboard";
    ViewBag.TitleIcon = "fa fa-warning";    

    if (!Request.IsAuthenticated)
    {
        Layout = null;
    }
}
<style>
    .error-icon {
        font-size: 45px;
        display: block;
        margin: auto;
        text-align: center;
        color: #F41B1B;
    }
</style>
<div id="page-content">
    <div class="genral-form">
        <div class="row">
            <div class="col-lg-12">
                <div class="panel">
                    <div class="panel-body">
                        <span class="error-icon"><i class="fa fa-warning"></i></span>
                        <h1 class="text-danger" style="text-align: center;">Error.</h1>
                        <p class="text-danger" style="text-align: center;">An error occurred while processing your request.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
